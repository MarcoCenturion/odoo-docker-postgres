<odoo>
    <record id="view_tst_wizard_form" model="ir.ui.view">
        <field name="name">tst.wizard.form</field>
        <field name="model">tst.wizard</field>
        <field name="arch" type="xml">
            <form string="Wizard para procesar TST">
                <group>
                    <field name="tst_content"/>
                </group>
                <footer>
                    <button string="Procesar" type="object" name="process_tst_content" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

